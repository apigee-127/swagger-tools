{
  "apiVersion": "1.0",
  "apis": [
    {
      "operations": [
        {
          "authorizations": {},
          "method": "GET",
          "nickname": "Weather_getWeather",
          "parameters": [
            {
              "name": "location",
              "paramType": "query",
              "description": "The MSN Weather location search string.",
              "required": true,
              "type": "string"
            },
            {
              "name": "unit",
              "paramType": "query",
              "description": "The unit, either 'C' or 'F'.",
              "enum": ["C", "F"],
              "defaultValue": "F",
              "required": true,
              "type": "string"
            }
          ],
          "responseMessages": [
            {
              "code": 400,
              "message": "Invalid request.",
              "responseModel": "Error"
            },
            {
              "code": 200,
              "message": "Successful request.",
              "responseModel": "Weather"
            }
          ],
          "notes": "Returns the current weather for the requested location using the requested unit.",
          "type": "Weather"
        }
      ],
      "path": "/weather"
    }
  ],
  "basePath": "http://localhost:3000/api",
  "models": {
    "CurrentWeather": {
      "id": "CurrentWeather",
      "properties": {
        "temperature": {
          "type": "string"
        },
        "skycode": {
          "type": "string"
        },
        "skytext": {
          "type": "string"
        },
        "date": {
          "type": "string"
        },
        "observationtime": {
          "type": "string"
        },
        "observationpoint": {
          "type": "string"
        },
        "feelslike": {
          "type": "string"
        },
        "humidity": {
          "type": "string"
        },
        "winddisplay": {
          "type": "string"
        },
        "day": {
          "type": "string"
        },
        "shortday": {
          "type": "string"
        },
        "windspeed": {
          "type": "string"
        },
        "imageUrl": {
          "type": "string"
        }

      },
      "required": ["temperature", "skycode", "skytext", "date", "observationtime", "observationpoint", "feelslike", "humidity", "winddisplay", "day", "shortday", "windspeed", "imageUrl"]
    },
    "Error": {
      "id": "Error",
      "properties": {
        "message": {
          "type": "string"
        }
      },
      "required": ["message"]
    },
    "Forecast": {
      "id": "Forecast",
      "properties": {
        "low": {
          "type": "string"
        },
        "high": {
          "type": "string"
        },
        "skycodeday": {
          "type": "string"
        },
        "skytextday": {
          "type": "string"
        },
        "date": {
          "type": "string"
        },
        "day": {
          "type": "string"
        },
        "shortday": {
          "type": "string"
        },
        "precip": {
          "type": "string"
        }
      },
      "required": ["low", "high", "skycodeday", "skytextday", "date", "day", "shortday", "precip"]
    },
    "Location": {
      "id": "Location",
      "properties": {
        "name": {
          "type": "string"
        },
        "zipcode": {
          "type": "string"
        },
        "lat": {
          "type": "string"
        },
        "long": {
          "type": "string"
        },
        "timezone": {
          "type": "string"
        },
        "alert": {
          "type": "string"
        },
        "degreetype": {
          "type": "string",
          "enum": ["C", "F"]
        },
        "imagerelativeurl": {
          "type": "string"
        }
      },
      "required": ["name", "lat", "long", "timezone", "degreetype"]
    },
    "Weather": {
      "id": "Weather",
      "properties": {
        "location": {
          "$ref": "Location"
        },
        "current": {
          "$ref": "CurrentWeather"
        },
        "forecast": {
          "type": "array",
          "items": {
            "$ref": "Forecast"
          }
        }
      },
      "required": ["current", "location", "forecast"]
    }
  },
  "produces": ["application/json"],
  "resourcePath": "/weather",
  "swaggerVersion": "1.2"
}
