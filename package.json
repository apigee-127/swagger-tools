{
  "name": "swagger-tools",
  "version": "1.0.0",
  "description": "Various tools for using and integrating with Swagger.",
  "main": "src/index.js",
  "scripts": {
    "test:unit": "mocha $(find test -type f \\( -iname 'test-*.js' ! -iname 'test-specs-browser.js' \\))",
    "test:unit:coverage": "nyc npm run test:unit",
    "clean:browser": "rimraf browser/*.js",
    "clean:dist": "rimraf dist",
    "build:test": "webpack --config webpack/test.config.js",
    "build:dev-standalone": "rimraf dist/*standalone.js && cross-env webpack --config webpack/dev-standalone.config.js",
    "build:prod-standalone": "cross-env NODE_ENV=production webpack --config webpack/prod-standalone.config.js",
    "build": "run-s clean:dist build:test build:dev-standalone build:prod-standalone",
    "jshint": "jshint --reporter=node_modules/jshint-stylish ./test/**/*.js ./src/**/*.js ./bin/swagger-tools",
    "test:karma": "node ./test/karma",
    "runner": "run-s jshint style test:unit:coverage build test:karma",
    "style": "eslint .",
    "style:fix": "npm run style -- --fix"
  },
  "author": {
    "name": "Jeremy Whitlock",
    "email": "jwhitlock@apache.org",
    "url": "https://github.com/whitlockjc"
  },
  "bugs": {
    "url": "https://github.com/apigee-127/swagger-tools/issues"
  },
  "homepage": "https://github.com/apigee-127/swagger-tools",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/apigee-127/swagger-tools.git"
  },
  "keywords": [
    "api",
    "connect",
    "middleware",
    "swagger"
  ],
  "bin": {
    "swagger-tools": "./bin/swagger-tools"
  },
  "files": [
    "bin",
    "index.js",
    "LICENSE",
    "lib",
    "middleware",
    "schemas"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-loader": "^8.0.6",
    "connect": "^3.4.0",
    "cross-env": "^7.0.0",
    "del": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jshint": "^2.11.0",
    "jshint-stylish": "^2.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^6.2.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "rimraf": "^3.0.1",
    "run-sequence": "^2.0.0",
    "supertest": "^3.0.0",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "async": "^2.5.0",
    "body-parser": "1.18.2",
    "commander": "~2.11.0",
    "debug": "^3.1.0",
    "js-yaml": "^3.3.1",
    "json-refs": "3.0.1",
    "lodash": "^4.17.4",
    "multer": "^1.1.0",
    "parseurl": "^1.3.0",
    "path-to-regexp": "^2.0.0",
    "qs": "^6.0.3",
    "serve-static": "^1.10.0",
    "spark-md5": "^3.0.0",
    "superagent": "^3.5.2",
    "swagger-converter": "^0.1.7",
    "traverse": "^0.6.6",
    "z-schema": "^3.15.4",
    "webpack-merge": "^4.2.2"
  }
}
